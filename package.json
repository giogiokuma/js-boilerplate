{
  "name": "js-boilerplate",
  "version": "0.0.1",
  "description": "react application boilerplate",
  "author": "taichi",
  "license": "MIT",
  "homepage": "https://github.com/taichi/js-boilerplate",
  "private": true,
  "scripts": {
    "build": "run-s lint compile test",
    "clear": "rimraf dist coverage .nyc_output",
    "lint": "eslint src/ test/",
    "compile": "cross-env NODE_ENV=production webpack --progress --profile --config config/webpack.js",
    "test": "cross-env NODE_ENV=test NODE_PATH=src ava --verbose",
    "cover": "nyc --reporter=html --reporter=text yarn test",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.js",
    "watch": "chokidar test/**/*.js -c \"cross-env NODE_ENV=test NODE_PATH=src ava --tap {path} | tap-notify\"",
    "postinstall": "flow-typed install"
  },
  "dependencies": {
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-css-modules": "^3.3.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.0.3",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.18.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "css-module-flow": "^1.0.0",
    "cssnano": "^3.9.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-security": "^1.3.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.56.0",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.24.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.1.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "prettier": "^1.7.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "tap-notify": "^1.0.0",
    "testdouble": "^3.2.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "ava": {
    "files": "test/**/*.test.js",
    "source": "src/**/*.{js,jsx}",
    "babel": "inherit",
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/global.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taichi/js-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/taichi/js-boilerplate/issues"
  }
}
