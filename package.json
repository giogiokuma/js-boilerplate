{
  "name": "js-boilerplate",
  "version": "0.0.1",
  "description": "react application boilerplate",
  "author": "taichi",
  "license": "MIT",
  "homepage": "https://github.com/taichi/js-boilerplate",
  "private": true,
  "scripts": {
    "build": "run-s lint compile test",
    "clear": "rimraf dist coverage .nyc_output",
    "lint": "eslint src/ test/",
    "compile": "cross-env NODE_ENV=production webpack --progress --profile --config config/webpack.js",
    "test": "cross-env NODE_ENV=test NODE_PATH=src ava --verbose",
    "cover": "nyc --reporter=html --reporter=text yarn test",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.js",
    "watch": "chokidar test/**/*.js -c \"cross-env NODE_ENV=test NODE_PATH=src ava --tap {path} | tap-notify\"",
    "postinstall": "flow-typed install"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-css-modules": "^2.2.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^2.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.18.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "css-module-flow": "^1.0.0",
    "cssnano": "^3.9.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-prettier": "^1.7.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-flowtype": "^2.32.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-security": "^1.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.44.2",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.24.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.0",
    "nyc": "^10.0.0",
    "postcss": "^5.2.8",
    "postcss-loader": "^1.2.1",
    "postcss-scss": "^0.4.0",
    "precss": "^1.4.0",
    "prettier": "^1.1.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.5.4",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.16.1",
    "tap-notify": "^1.0.0",
    "testdouble": "^2.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "ava": {
    "files": "test/**/*.test.js",
    "source": "src/**/*.{js,jsx}",
    "babel": "inherit",
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/global.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taichi/js-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/taichi/js-boilerplate/issues"
  }
}
