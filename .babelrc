{
    "sourceMaps": "inline",
    "presets": [
        "flow",
        "react-app"
    ],
    "plugins": [
        [
            "react-css-modules",
            {
                "filetypes": {
                    ".scss": {
                        "syntax": "postcss-scss"
                    }
                },
                "generateScopedName": "[hash:base64]"
            }
        ]
    ],
    "env": {
        "test": {
            "plugins": []
        }
    }
}
